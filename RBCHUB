local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("RBC > Community")

local serv = win:Server("RBC HUB", "")

--Home
local lbls = serv:Channel("Home/Info")

lbls:Label("< Hello! Welcome to RBC Hub >")
lbls:Label("● In our discord server you can find:")
lbls:Label("● 1 ● Economy [Work, Rob, Buy]")
lbls:Label("● 2 ● Friendly Chat [Speak in chat to get giveaway role]")
lbls:Label("● 3 ● Giveaways [Cookies, Robux, Accounts and other]")
lbls:Label("● 4 ● Usefull Information")
lbls:Label("< How to get giveaway role? >")
lbls:Label("To get role you need to get 5 level, or invite 5 people")
lbls:Label("in our server. To get 5 lvl you need talk in chat(it take 30m)")
lbls:Label("< discord.gg/rbc >")
lbls:Label("< Good luck ! >")

